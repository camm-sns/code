<?xml version="1.0" ?>
<root>
  <!-- model b0+b1*E  +  e0*exp(-e1*Q^2)*Elastic(E)  +  c0*Resolution(E)xSimulated(Q,E) -->
  <FFParams>
    <FFParam init="1.0" maximum="0.1" minimum="0.0" name="b0" tolerance="0.01"/>
    <FFParam init="0.0" maximum="0.0" minimum="0.0" name="b1" tolerance="0.01"/>
    <FFParam init="1.0" maximum="1000" minimum="0.0" name="e0" tolerance="0.01"/>
    <FFParam init="0.001" maximum="10" minimum="0.001" name="e1" tolerance="0.01"/>
    <FFParam init="1.0" maximum="1000" minimum="0.001" name="c0" tolerance="0.01"/>
  </FFParams>
  <FFTemplate>
b0=_b0_(%-14.6e); b1=_b1_(%-14.6e); e0=_e0_(%-14.6e); e1=_e1_(%-14.6e); c0=_c0_(%-14.6e)
</FFTemplate>
</root>
